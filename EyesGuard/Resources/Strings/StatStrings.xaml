<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:EyesGuard.Resources.Strings"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="Strings.EyesGuard.Stats.Short">Short-break</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Stats.Long">Long-break</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Stats.Paused">Pauses</sys:String>

    <sys:String x:Key="Strings.EyesGuard.Stats.ShortCount">Short-breaks:</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Stats.LongComplatedCount">Completed Long-breaks:</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Stats.LongFailedCount">Failed Long-breaks:</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Stats.PauseCount">Temporary pauses:</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Stats.StopCount">Stops:</sys:String>

</ResourceDictionary>
