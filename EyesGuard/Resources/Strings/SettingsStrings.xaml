<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:EyesGuard.Resources.Strings"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="Strings.EyesGuard.Settings.TimeSection">Time Settings</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Settings.UserSection">User Settings</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Settings.Stats">Stats</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Settings.Prefrences">Preferences</sys:String>

    <sys:String x:Key="Strings.EyesGuard.Settings.ShortGap">Time gap between two Short-break:</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Settings.LongGap">Time gap between two Long-break:</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Settings.ShortDuration">Short-break duration:</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Settings.LongDuration">Long-break duration:</sys:String>

    <sys:String x:Key="Strings.EyesGuard.Settings.Separator.HourAnd">Hour(s) and</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Settings.Separator.MinutesAnd">Minute(s) and</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Settings.Separator.Second">Second(s)</sys:String>

    <sys:String x:Key="Strings.EyesGuard.Settings.ForceUser">Force me to break</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Settings.OnlyOneShortBreak">Show only one short-break between two long-breaks.</sys:String>
