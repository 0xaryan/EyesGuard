    <sys:String x:Key="Strings.EyesGuard.Settings.AlertBeforeLongBreak">Notify me 1 minute before long-break</sys:String>

    <sys:String x:Key="Strings.EyesGuard.Settings.StoreStats">Store stats</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Settings.ClearStatsText">Remove history of stats:</sys:String>
    <sys:String x:Key="Strings.EyesGuard.Settings.ClearStats">Remove data</sys:String>

    <sys:String x:Key="Strings.EyesGuard.Settings.UseWindowsDPIScaling">Use windows DPI Scaling</sys:String>

    <sys:String x:Key="Strings.EyesGuard.Settings.SystemIdle">Pause Eyes Guard when system detected as idle</sys:String>

    <sys:String x:Key="Strings.EyesGuard.Settings.SaveSettings">Save changes</sys:String>

